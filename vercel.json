{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "next-js-streaming-example/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/app/(.*)",
      "dest": "/next-js-streaming-example/$1"
    },
    {
      "src": "/app",
      "dest": "/next-js-streaming-example"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}